{
  "nodejs": {
    "type": "domain",
    "enforcement": "suggest",
    "priority": "low",
    "promptTriggers": {
      "keywords": [
        "node.js",
        "nodejs",
        "async/await",
        "process.env",
        "require(",
        "module.exports"
      ],
      "intentPatterns": [
        "create.*node.*server",
        "implement.*async.*function",
        "handle.*error",
        "create.*service"
      ]
    },
    "fileTriggers": {
      "pathPatterns": [
        "**/services/**/*.ts",
        "**/utils/**/*.ts",
        "**/lib/**/*.ts"
      ],
      "contentPatterns": [
        "async.*function",
        "await.*",
        "process\\.env"
      ]
    }
  }
}
